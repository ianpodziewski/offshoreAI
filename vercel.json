{
    "version": 2,
    "builds": [
      {
        "src": "api/*.py",
        "use": "@vercel/python@3",
        "config": { 
          "runtime": "python3.11",
          "pythonVersion": "3.11"
        }
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }