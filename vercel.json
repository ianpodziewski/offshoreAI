{
    "version": 2,
    "builds": [
      {
        "src": "api/split_pdf.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/split_pdf_cli.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/api/split_pdf", "dest": "api/split_pdf.py" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }